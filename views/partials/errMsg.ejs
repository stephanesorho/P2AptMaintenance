<% if (err) {%>

    <div id="errMsg" class="alert alert-<%=type ? type : " alert" %> alert-dismissible fade show" role=<%=type ? type
            : "alert" %>
            >
            <%= err %>
                <button type="button" class="btn-close" data-dismiss="alert" aria-label="Close">
                </button>
    </div>
    <% } %>